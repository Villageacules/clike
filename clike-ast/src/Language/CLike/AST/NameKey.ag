-------------------------------------------------------------------------------------------
-- Key to be used in lookup
-------------------------------------------------------------------------------------------

ATTR Name AGItfName [ | | mbKey: {Maybe LexString} ]

SEM Name
  | Id			lhs			.	mbKey		=	Just @id
  | * - Id		lhs			.	mbKey		=	Nothing

